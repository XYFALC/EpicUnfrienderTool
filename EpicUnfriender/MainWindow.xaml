<Window x:Class="EpicUnfriender.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpicUnfriender"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400" ResizeMode="NoResize">
    <Grid Height="150" Width="350">
        <Button x:Name="Start" Content="Start" HorizontalAlignment="Left" Height="21" Margin="59,91,0,0" VerticalAlignment="Top" Width="73" BorderBrush="#FFC35151" RenderTransformOrigin="0.457,0.761" Click="Button_Click" Background="#FFF5F5FF"/>
        <Button x:Name="Stop" Content="Stop" HorizontalAlignment="Left" Height="21" Margin="216,91,0,0" VerticalAlignment="Top" Width="74" Background="#FFF3F3F3" BorderBrush="#FFC35151" RenderTransformOrigin="0.457,0.761"/>
        <TextBox x:Name="LogBox" HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" Height="76"/>

    </Grid>
</Window>
